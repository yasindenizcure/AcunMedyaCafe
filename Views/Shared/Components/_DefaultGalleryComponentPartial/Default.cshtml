@model List<AcunMedyaCafe.Entities.Gallery>
<section class="gallery-area section-gap" id="gallery">
	<div class="container">
		<div class="row d-flex justify-content-center">
			<div class="menu-content pb-60 col-lg-10">
				<div class="title text-center">
					<h1 class="mb-10">Galeri</h1>
					<p>Kafemizden birtakım fotoğraflar..</p>
				</div>
			</div>
		</div>
		<div class="row">
			@foreach (var item in Model)
			{
				<a href="@item.PhotoUrl" class="img-pop-home">
					<img class="img-fluid" src="@item.PhotoUrl" style="width:550px;height:400px;" alt="">
				</a>
			}
</div>
		</div>
</section>